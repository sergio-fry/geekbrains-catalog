<div id="<%= dom_id item %>">
  <p>
  <% item.images.each do |image| %>
    <%= image_tag image.variant(resize_to_fill: [200, 200]) %>
  <% end %>
  </p>
  <p>
    <strong>Title:</strong>
    <%= item.title %>
  </p>

  <p>
    <strong>Category:</strong>
    <%= item.category_id %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= item.price %>
  </p>

</div>
